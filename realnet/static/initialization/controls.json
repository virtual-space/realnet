            "name": "GeneralTab",
            "base": "TabCtrl",
            "attributes": {
                "controls": [
                    {
                        "name": "Name",
                        "type": "EditCtrl",
                        "attributes": {
                            "target": "name",
                            "field_name": "name",
                            "order": "1"
                        }
                    },
                    {
                        "name": "Status",
                        "type": "SelectCtrl",
                        "attributes": {
                            "target": "status",
                            "field_name": "status",
